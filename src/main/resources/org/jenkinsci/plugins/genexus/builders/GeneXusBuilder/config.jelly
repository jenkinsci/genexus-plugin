<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:f="/lib/form" xmlns:c="/lib/credentials">

    <f:entry title="${%GeneXus Installation}" field="gxInstallationId">
        <f:select />
    </f:entry>
    
    <f:entry field="kbPath" title="${%Knowledge Base Path}">
        <f:textbox default="KBpath"/>
    </f:entry>

    <f:entry field="kbVersion" title="${%Version}">
        <f:textbox/>
    </f:entry>

    <f:entry field="kbEnvironment" title="${%Environment}">
        <f:textbox/>
    </f:entry>

    <f:advanced align="left" style="text-decoration:bold" title="Advanced Options">
         <f:entry field="kbDbServerInstance" title="${%SQL Server Name}">
            <f:textbox />
        </f:entry>

        <f:entry field="kbDbCredentialsId" title="${%SQL Server Credentials}">
            <c:select checkMethod="post"/>
        </f:entry>

        <f:entry field="forceRebuild" title="${%Force rebuilding all objects}">
            <f:checkbox default="false" />
        </f:entry>
    </f:advanced>
</j:jelly>
